{
  "name": "obedjs",
  "version": "1.0.0",
  "main": "src/app.js",
  "scripts": {
    "generate": "ts-node ./scripts/generate.ts",
    "start": "npm-run-all build start:prod",
    "start:prod": "node dist/app.js",
    "start:dev": "npm-run-all build start:dev-only",
    "start:dev-only": "nodemon src/app.ts",
    "build": "rimraf dist && tsc",
    "test": "jest --config jest.config.js --watchAll",
    "test:e2e": "jest --config jest.e2e.config.js --watchAll"
  },
  "keywords": ["nodejs", "expressjs", "obedjs"],
  "author": "",
  "license": "MIT",
  "description": "A TypeScript-Powered Framework Built on Express.js for Scalable and Robust Applications",
  "dependencies": {
    "class-validator": "^0.14.1",
    "express": "^4.19.2",
    "inquirer": "^9.2.22",
    "mongoose": "^8.4.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
